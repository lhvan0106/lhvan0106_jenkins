pipeline {
    agent any 
    stages {
        
        stage('Build') {
            steps {
                dir("restapi") {
                    bat "mvn clean install -Dmaven.test.skip=true"
                }
            }
        }

        stage('Copy File') { 
            steps {
                bat "copy .\\restapi\\target\\restapi-0.0.1-SNAPSHOT.jar E:\\restapi.jar"
            }
        }
    }
}