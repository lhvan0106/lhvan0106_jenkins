pipeline {
    agent any 
    stages {
        
        stage('Build') {
            steps {
                dir("lhvan0106_jenkins\\restapi") {
                    bat "mvn clean install -Dmaven.test.skip=true"
                }
            }
        }

        stage('Copy File') { 
            steps {
                bat "copy .\\lhvan0106_jenkins\\restapi\\target\\restapi-0.0.1-SNAPSHOT.jar E:\\restapi.jar"
            }
        }
    }
}